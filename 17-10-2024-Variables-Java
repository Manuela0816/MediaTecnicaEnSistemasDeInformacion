class HelloWorld {
    public static void main(String[] args) {
        //Enteras
        int entera = 14;
        Integer entero;
        System.out.println("El rango de un entero es: -2,147,483,648 a -2,147,483,647");
        System.out.println("Ejemplo a la variable es:" + entera);
       
        
        //Long
        
        long enteroGrande = 20;
        Long enteroGrandeobject;
        System.out.println("Ejemplo a la variable es:" + enteroGrande);
        System.out.println("El rango de un long es: -9,223,372,036,854,775,808 a 9,223,372,036,854,775,807 ");
        
        
        //Decimal
        float decimal = 4;
        Float decimalObject;
        System.out.println("El rango de un float es: 1.4e-045 to 3.4e+038");
        System.out.println("Ejemplo a la variable es:" +decimal);
        
        
        double decimal2 = 8;
        Double decimal2Object;
        System.out.println("El rango de un double es: 4.9e-324 to 1.8e+308");
        System.out.println("Ejemplo a la variable es:" + decimal2);
        
        short enteroPequenno = 3;
        Short enteroShort;
        System.out.println("Ejemplo a la variable es:" + enteroPequenno);
        System.out.println("Elrango de un short es: -32,768 a 32,767");
        
        byte enteroMuyPequenno = 1;
        Byte enteroMuyPequennoObject;
        System.out.println("Ejemplo a la variable es:" + enteroMuyPequenno);
        System.out.println("El rango de byte es: -128 a 127");
        
        
        //Boolean
        boolean bool = true;
        System.out.println("Ejemplo a la variable es:" +bool);
        Boolean booleanObject;
        System.out.println("El boolean puede ser Verdadero o Falso");
        
        
        //Cadena de caracteres
        String cadena = "Gracias Beyonce";
        System.out.println("El string puede imprimir cadenas de palabras");
        System.out.println("Ejemplo a la variable es:" + cadena);
        
        
        //Caracter
        char caracter = '*';
        Character caracterObject;
        System.out.println("El char puede tener un simbolo, una letra, o un valor numerico que represente una letra en UniCode");
        System.out.println("Ejemplo a la variable es:" + caracter);
        
        
        //Object
        Object objeto;
        System.out.println("El objeto puede almacenar cualquier objeto y este objeto puede tomar tipo de dato");
        
    }
}
